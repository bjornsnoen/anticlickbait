    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "execa": "^7.1.1",
    "glob": "^9.2.1",
    "prettier": "^2.7.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "web-ext": "^7.11.0"
  }
}
